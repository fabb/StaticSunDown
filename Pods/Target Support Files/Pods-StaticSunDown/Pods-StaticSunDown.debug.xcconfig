FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/UIWebView-Markdown" "$PODS_CONFIGURATION_BUILD_DIR/sundown"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/UIWebView-Markdown/UIWebView_Markdown.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/sundown/sundown.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "UIWebView_Markdown" -framework "sundown"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
